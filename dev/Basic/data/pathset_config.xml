<pathset enabled="true" mode="generation"> <!-- currently, "normal" and "generation" are supported modes --> 
	<bulk_generation_output_file_name value="pathset_bulk.csv" />
	<pathset_database database="fm_local_path_choice"  credentials="fm_local_path_choice" /> <!--unused for now-->
	<tables singlepath_table="path_fake" realtime_traveltime="realtime_travel_time" default_traveltime = "link_default_travel_time_corrected_he_v2"/>
	<functions pathset="get_path_set_fake" travel_time="upsert_realtime" />
	<travel_time_update interval="600" alpha="0.5" />
	<recursive_pathset_generation value="true" />
	<reroute_enabled value="true" />
	<subtrip_travel_metrics_output enabled="true" file="subtrip_level_travel_metrics_for_preday.csv" />    	
	<utility_parameters>
		<bTTVOT value="-0.01373"/>
		<bCommonFactor value="1.0"/>
		<bLength value="-0.001025"/>
		<bHighway value="0.00052"/>
		<bCost value="0.0"/>
		<bSigInter value="-0.13"/>
		<bLeftTurns value="0.0"/>
		<bWork value="0.0"/>
		<bLeisure value="0.0"/>
		<highwayBias value="0.5"/>
		<minTravelTimeParam value="0.879"/>
		<minDistanceParam value="0.325"/>
		<minSignalParam value="0.256"/>
		<maxHighwayParam value="0.422"/>
	</utility_parameters>
	<path_generators>																
		<k_shortest level="3" />														
		<link_elimination types="default,highway" />	<!-- unused for now. only default and highway are used in the code-->									
		<random_perturbation modes="time" iteration="20" uniform_range="1,1000" /> <!-- for 'modes' attribute, only "time" is supported for now -->		
	</path_generators>
</pathset>

